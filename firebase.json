{
    "hosting": {
        "public": "build",
        "src": "../src/*",
        "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
        "rewrites": [{
            "source": "**",
            "destination": "/index.html"
        }],
        "headers": [{
            "source": "../src/serviceWorker.js",
            "headers": [{ "key": "Cache-Control", "value": "public, max-age=2592000" }]
        }]
    }
}